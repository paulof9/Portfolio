{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build && echo '✅ Build terminou. Conteúdo do src:' && ls -lh ./app/blueprints/main/static/src"
}
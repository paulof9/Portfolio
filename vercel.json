{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "echo '--- VERCEL CUSTOM BUILD COMMAND EXECUTING ---' && npm run build"
}